    <textures file="gui.png" format="RGBA">
        <texture name="checkerboard" x="56" y="106" width="8" height="8" tiled="true"/>
        <texture name="-positionbar-horizontal-0" x="124" y="68" width="32" height="1" tiled="true"/>
        <texture name="-positionbar-horizontal-1" x="122" y="68" width="32" height="1" tiled="true"/>
        <texture name="-positionbar-horizontal-2" x="120" y="68" width="32" height="1" tiled="true"/>
        <texture name="-positionbar-horizontal-3" x="118" y="68" width="32" height="1" tiled="true"/>
        <texture name="-positionbar-vertical-0" x="156" y="6" width="1" height="40" tiled="true"/>
        <texture name="-positionbar-vertical-1" x="156" y="4" width="1" height="40" tiled="true"/>
        <texture name="-positionbar-vertical-2" x="156" y="2" width="1" height="40" tiled="true"/>
        <texture name="-positionbar-vertical-3" x="156" y="0" width="1" height="40" tiled="true"/>
        <texture name="-icon-moveup" x="74" y="19" width="12" height="8" center="true"/>
        <texture name="-icon-movedown" x="87" y="19" width="12" height="8" center="true"/>
        <texture name="-icon-apply" x="86" y="37" width="13" height="10" center="true" inset="0,1,0,-1"/>
        <texture name="-icon-clone" x="84" y="48" width="11" height="12" center="true"/>
        <texture name="-icon-jump" x="83" y="61" width="14" height="9" center="true"/>
        <texture name="-icon-add" x="48" y="19" width="12" height="12" center="true"/>
        <texture name="-icon-remove" x="61" y="19" width="12" height="12" center="true"/>
        <texture name="-icon-select" x="105" y="68" width="12" height="12" center="true"/>

        <cursor name="cursor.dragSelect" x="90" y="82" width="16" height="16" hotSpotX="5" hotSpotY="5"/>
        
        <alias name="textureviewer.background" ref="checkerboard" border="64" inset="64" sizeOverwriteH="0" sizeOverwriteV="0"/>

        <select name="positionbar-horizontal">
            <animation timeSource="now" if="animatedPositionBars">
                <frame duration="200" ref="-positionbar-horizontal-0"/>
                <frame duration="100" ref="-positionbar-horizontal-1"/>
                <frame duration="100" ref="-positionbar-horizontal-2"/>
                <frame duration="200" ref="-positionbar-horizontal-3"/>
                <frame duration="100" ref="-positionbar-horizontal-2"/>
                <frame duration="100" ref="-positionbar-horizontal-1"/>
            </animation>
            <alias ref="-positionbar-horizontal-0"/>
        </select>

        <select name="positionbar-vertical">
            <animation timeSource="now" if="animatedPositionBars">
                <frame duration="200" ref="-positionbar-vertical-0"/>
                <frame duration="100" ref="-positionbar-vertical-1"/>
                <frame duration="100" ref="-positionbar-vertical-2"/>
                <frame duration="200" ref="-positionbar-vertical-3"/>
                <frame duration="100" ref="-positionbar-vertical-2"/>
                <frame duration="100" ref="-positionbar-vertical-1"/>
            </animation>
            <alias ref="-positionbar-vertical-0"/>
        </select>
        
        <grid name="mainmenu.background" weightsX="1" weightsY="1,0,0" border="0,0,4,0">
            <alias ref="none"/>
            <alias ref="panel.separator"/>
            <alias ref="none" sizeOverwriteV="1"/>
        </grid>

        <grid name="statusbar.background" weightsX="1" weightsY="0,0,1" border="5,5,2,5">
            <alias ref="none" sizeOverwriteV="1"/>
            <alias ref="panel.separator"/>
            <alias ref="none"/>
        </grid>

        <grid name="-white-frame" weightsX="0,1,0" weightsY="0,1,0">
            <alias ref="white"/>
            <alias ref="white"/>
            <alias ref="white"/>
            <alias ref="white"/>
            <alias ref="none"/>
            <alias ref="white"/>
            <alias ref="white"/>
            <alias ref="white"/>
            <alias ref="white"/>
        </grid>
        <select name="flashImage">
            <animation timeSource="flashing" count="1" if="flashing">
                <repeat count="5">
                    <frame ref="-white-frame" duration="100" tint="red"/>
                    <frame ref="-white-frame" duration="100" tint="blue"/>
                </repeat>
                <frame ref="none" duration="100"/>
            </animation>
            <alias ref="-white-frame" tint="red"/>
        </select>

        <composed name="addbutton.overlay">
            <select>
                <alias ref="-icon-add" if="disabled" tint="#6fff"/>
                <alias ref="-icon-add" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-add"/>
            </select>
            <alias ref="button.overlay"/>
        </composed>

        <composed name="removebutton.overlay">
            <select>
                <alias ref="-icon-remove" if="disabled" tint="#6fff"/>
                <alias ref="-icon-remove" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-remove"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <alias name="button.opDeleteNode.overlay" ref="removebutton.overlay"/>
        <alias name="button.opNewNode.overlay" ref="addbutton.overlay"/>

        <composed name="button.opMoveNodeUp.overlay">
            <select>
                <alias ref="-icon-moveup" if="disabled" tint="#6fff"/>
                <alias ref="-icon-moveup" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-moveup"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <composed name="button.opMoveNodeDown.overlay">
            <select>
                <alias ref="-icon-movedown" if="disabled" tint="#6fff"/>
                <alias ref="-icon-movedown" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-movedown"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <composed name="button.opCloneNode.overlay">
            <select>
                <alias ref="-icon-clone" if="disabled" tint="#6fff"/>
                <alias ref="-icon-clone" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-clone"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <composed name="applybutton.overlay">
            <select>
                <alias ref="-icon-apply" if="disabled" tint="#6fff"/>
                <alias ref="-icon-apply" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-apply"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <composed name="jumpbutton.overlay">
            <select>
                <alias ref="-icon-jump" if="disabled" tint="#6fff"/>
                <alias ref="-icon-jump" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-jump"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>
        
        <alias name="--icon-flash" ref="-white-frame" sizeOverwriteH="12" sizeOverwriteV="12"/>
        <alias name="-icon-flash" ref="--icon-flash" center="true"/>
        <composed name="flashbutton.overlay">
            <select>
                <alias ref="-icon-flash" tint="#6fff" if="disabled"/>
                <alias ref="-icon-flash" tint="red" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-flash" tint="red"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>

        <composed name="selectbutton.overlay">
            <select>
                <alias ref="-icon-select" if="disabled" tint="#6fff"/>
                <alias ref="-icon-select" if="armed" inset="1,1,-1,-1"/>
                <alias ref="-icon-select"/>
            </select>
            <alias ref="vbutton.overlay"/>
        </composed>
    </textures>

    <theme name="themetreepane" ref="dialoglayout">
        <param name=""><image>panel.*</image></param>
        <param name="border"><border>0,0,5,0</border></param>
